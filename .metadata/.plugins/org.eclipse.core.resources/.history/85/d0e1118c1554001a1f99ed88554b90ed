import java.util.Arrays;

public class Methoden1_25 {

	public static int gross (int arre) {
		int[]arr= {2,3,4};
		Arrays.sort(arr);
		//int n = groesster;
		//System.out.println(groesster);
		return arre;
	}


	public static void main(String[] args) {
		int i = 1;
		gross(i);
		System.out.println(i);
		
}}
