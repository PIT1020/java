import java.util.Scanner;

public class VokabelTrainer_24 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner eingabe = new Scanner(System.in);
		System.out.println("Beginn der Eingabe:");
		// String Vok1 = eingabe.nextLine();
		int columns = 2;
		int rows = 2;
		String[][] Vok1 = new String[columns][rows];
		Vok1[0][0] = "Baum";
		Vok1[0][1] = "Tree";
		Vok1[1][0] = "See";
		Vok1[1][1] = "Lake";
		System.out.println("1.Vokabel:" + " " + Vok1[0][0] + ":" + Vok1[0][1]);
		System.out.println("2.Vokabel:" + " " + Vok1[1][0] + ":" + Vok1[1][1]);
		System.out.println("Ende der Eingabe");
		System.out.println("Beginn des Trainings");
		for (int i = 0; i < rows; i++) {
			for (int k = 0; k < columns; k++) {
				System.out.print(Vok1[i]);
			}

		}
	}
}
