import java.util.Scanner;

public class ZweidimensionalesArray_27 {

	public static int printArray(int[][] zweiarray) {

		int ret = 100;

		// Ausgabe des Arrays
		System.out.println("Ausgabe des Arrays:");
		for (int i = 0; i < zweiarray.length; i++) {
			System.out.print(zweiarray[i] + "" + zweiarray[i] + " ");
		}

		// Vertauschen
		// System.out.println("Ausgabe des gespiegelten Arrays:");
		for (int i = 0; i < zweiarray.length / 2; i++) {
			int[] buffer = zweiarray[i];
			zweiarray[i] = zweiarray[zweiarray.length - 1 - i];
			zweiarray[zweiarray.length - 1 - i] = buffer;
		}
		System.out.println("");
		for (int i = 0; i < zweiarray.length; i++) {
			System.out.print(zweiarray[i] + "" + zweiarray[i] + " ");
		}
		System.out.println("");
		ret = 1;
		return ret;
	}

	public static int transponieren(int[][] tarray) {

		int ret = 101;
		for (int i = 0; i < tarray.length; i++) {
			for (int j = 0; j < tarray.length; j++) {
				int temp = tarray[i][j];
				tarray[i][j] = tarray[j][i];
				tarray[j][i] = temp;
			}
		}
		ret = 2;
		return ret;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner eingabe = new Scanner(System.in);
		int [][] zweiarray = new int [4][4];
		int zahl = 0;
		for (int i = 0; i < zweiarray.length; i++) {
			for (int j = 0; j < zweiarray.length; j++) {
				zahl = eingabe.nextInt();
				zweiarray[i][j]= zahl;
				System.out.print(zahl);
			}
		}

	}

}
